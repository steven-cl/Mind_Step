<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo"
    tools:context=".AdminUser.AsignarCitas">

    <View
        android:id="@+id/toppanel"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:background="@color/MenuColor"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        />

    <View
        android:id="@+id/topsombra"
        android:layout_width="wrap_content"
        android:layout_height="1.2dp"
        android:layout_gravity="center_horizontal"
        android:background="#494E4C"
        android:elevation="50dp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toppanel"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/bottompanel"
        android:layout_width="wrap_content"
        android:layout_height="89dp"
        android:background="@color/MenuColor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/bottomsombra"
        android:layout_width="wrap_content"
        android:layout_height="1.2dp"
        android:layout_above="@id/bottompanel"
        android:background="#494E4C"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/bottompanel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_gravity="center_horizontal"
        android:elevation="50dp"
        android:outlineAmbientShadowColor="@color/BackgroundColor"
        android:outlineSpotShadowColor="@color/black"
        tools:ignore="MissingConstraints"
        tools:targetApi="p" />

    <View
        android:id="@+id/perfil_clickable"
        android:layout_width="85dp"
        android:layout_height="79dp"
        android:layout_above="@id/perfil"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,MissingContraints" />

    <ImageView
        android:id="@+id/perfil"
        android:layout_width="68dp"
        android:layout_height="64dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="9dp"
        app:layout_constraintBottom_toBottomOf="@+id/perfil_clickable"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/perfil_clickable"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/usuario"
        android:importantForAccessibility="no" />

    <TextView
        android:id="@+id/textLogo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/identidad_extra_bold"
        android:text="@string/app_name"
        android:textColor="@color/verde"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="@+id/toppanel"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.037"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="79dp"
        android:layout_height="67dp"
        android:layout_marginStart="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/toppanel"
        app:layout_constraintStart_toStartOf="@+id/toppanel"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.538"
        app:srcCompat="@drawable/logo_icono"
        tools:ignore="ContentDescription" />


    <Button
        android:id="@+id/btnBuscar"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/fondo_verde_claro"
        android:text="@string/buscar_user"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/perfil_clickable" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="200dp"
        android:layout_height="48dp"
        android:layout_marginTop="36dp"
        android:textColor="@color/black"
        android:background="@drawable/fondo_verde_claro"
        app:layout_constraintEnd_toStartOf="@+id/btnBuscar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/topsombra"
        android:autofillHints="Buscar"
        android:hint="@string/buscar_user"
        android:inputType="text" />

    <GridLayout
        android:id="@+id/gridLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="40dp"
        android:alignmentMode="alignMargins"
        android:columnCount="3"
        android:columnOrderPreserved="false"
        android:rowCount="2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText">

        <!-- Elemento 1 -->
        <Button
            android:layout_row="0"
            android:layout_column="0"
            android:layout_gravity="fill"
            android:textColor="@color/black"
            android:background="@drawable/button_border"
            android:text="Nombre" />

        <!-- Elemento 2 -->
        <Button
            android:layout_row="0"
            android:layout_column="1"
            android:textColor="@color/black"
            android:layout_gravity="fill"
            android:background="@drawable/button_border"
            android:text="Apellido" />

        <!-- Elemento 3 -->
        <Button
            android:layout_row="0"
            android:layout_column="2"
            android:textColor="@color/black"
            android:layout_gravity="fill"
            android:background="@drawable/button_border"
            android:text="Elemento 3" />

        <!-- Elemento 4 -->
        <Button
            android:layout_row="1"
            android:layout_column="0"
            android:textColor="@color/black"
            android:layout_gravity="fill"
            android:background="@drawable/button_border"
            android:text="Elemento 4" />

        <!-- Elemento 5 -->
        <Button
            android:layout_row="1"
            android:layout_column="1"
            android:textColor="@color/black"
            android:layout_gravity="fill"
            android:background="@drawable/button_border"
            android:text="Elemento 5" />

        <!-- Elemento 6 -->
        <Button
            android:layout_row="1"
            android:layout_column="2"
            android:textColor="@color/black"
            android:layout_gravity="fill"
            android:background="@drawable/button_border"
            android:text="Elemento 6" />

    </GridLayout>

    <Button
        android:id="@+id/btnFichas"
        android:layout_width="200dp"
        android:layout_height="48dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/fondo_verde_claro"
        android:text="@string/fichas"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gridLayout2" />


    <Button
        android:id="@+id/btnAsignarCita"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/fondo_verde_claro"
        android:text="@string/asignar_cita"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnFichas" />

    <View
        android:id="@+id/view7"
        android:layout_width="1.5dp"
        android:layout_height="89dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/bottompanel"
        app:layout_constraintVertical_bias="0.166" />

    <View
        android:id="@+id/view8"
        android:layout_width="1.5dp"
        android:layout_height="89dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view4"
        app:layout_constraintTop_toTopOf="@+id/bottompanel"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/view9"
        android:layout_width="1.5dp"
        android:layout_height="89dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view5"
        app:layout_constraintTop_toTopOf="@+id/bottompanel"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/ajuste_imagen"
        android:layout_width="79dp"
        android:layout_height="79dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/bottompanel"
        app:layout_constraintStart_toEndOf="@+id/view9"
        app:layout_constraintTop_toTopOf="@+id/bottompanel"
        app:srcCompat="@drawable/ajuste"
        android:importantForAccessibility="no" />

    <ImageView
        android:id="@+id/regitro_imagen"
        android:layout_width="79dp"
        android:layout_height="79dp"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/view7"
        app:layout_constraintStart_toStartOf="@+id/bottompanel"
        app:layout_constraintTop_toTopOf="@+id/bottompanel"
        app:srcCompat="@drawable/registro" />

    <ImageView
        android:id="@+id/administrador_imagen"
        android:layout_width="79dp"
        android:layout_height="79dp"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/view8"
        app:layout_constraintStart_toEndOf="@+id/view7"
        app:layout_constraintTop_toTopOf="@+id/bottompanel"
        app:srcCompat="@drawable/administrador" />

    <ImageView
        android:id="@+id/citas_imagen"
        android:layout_width="79dp"
        android:layout_height="79dp"
        android:importantForAccessibility="no"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/view9"
        app:layout_constraintStart_toEndOf="@+id/view8"
        app:layout_constraintTop_toTopOf="@+id/bottompanel"
        app:srcCompat="@drawable/cita" />

    <View
        android:id="@+id/view4"
        android:layout_width="100dp"
        android:layout_height="89dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view7"
        app:layout_constraintTop_toTopOf="@+id/bottompanel"
        app:layout_constraintVertical_bias="0.526" />

    <View
        android:id="@+id/view5"
        android:layout_width="100dp"
        android:layout_height="89dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view8"
        app:layout_constraintTop_toTopOf="@+id/bottompanel"
        app:layout_constraintVertical_bias="0.526" />

    <View
        android:id="@+id/view6"
        android:layout_width="100dp"
        android:layout_height="89dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/bottompanel"
        app:layout_constraintHorizontal_bias="0.238"
        app:layout_constraintStart_toEndOf="@+id/view9"
        app:layout_constraintTop_toTopOf="@+id/bottompanel"
        app:layout_constraintVertical_bias="0.526" />

    <View
        android:id="@+id/view"
        android:layout_width="100dp"
        android:layout_height="89dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/bottompanel"
        app:layout_constraintVertical_bias="0.526" />

</androidx.constraintlayout.widget.ConstraintLayout>
